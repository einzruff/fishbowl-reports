	<queryString>
		<![CDATA[SELECT po.vendorContact, po.vendorSo, po.vendorId, po.currencyRate, currency.code AS currencyCode, currency.symbol,
	po.note, po.id AS recordid, COALESCE(UPPER(sysProperties.sysValue), 'US') AS qbVersion,

    /*Ship To*/
    po.shipToName, po.shipToAddress, po.shipToCity, shipState.code AS shipToState,
	po.shipToZip, shipCountry.name AS shipToCountry,

	/*Bill To*/
    po.remitToName, po.remitAddress, po.remitCity, billState.code AS billToState,
    po.remitZip, billCountry.name AS billToCountry,

	CASE COALESCE(UPPER(sysProperties.sysValue), 'US')
		WHEN 'CA' THEN ROUND(SUM(poItem.unitCost * poItem.qtyToFulfill * poItem.taxRate) / po.currencyRate , 2)
		WHEN 'AU' THEN ROUND(SUM(ROUND(poItem.unitCost * poItem.qtyToFulfill * poItem.taxRate / po.currencyRate, 2)), 2)
		ELSE 0.0
	END AS currencyTaxTotal,

	CASE COALESCE(UPPER(sysProperties.sysValue), 'US')
		WHEN 'CA' THEN SUM(poItem.unitCost * poItem.qtyToFulfill * poItem.taxRate) / po.currencyRate
		WHEN 'AU' THEN SUM(poItem.unitCost * poItem.qtyToFulfill * poItem.taxRate / po.currencyRate)
		ELSE 0.0
	END AS currencyTaxTotalNotRounded,

	CASE COALESCE(UPPER(sysProperties.sysValue), 'US')
		WHEN 'US' THEN SUM(ROUND(poItem.unitCost / po.currencyRate * poItem.qtyToFulfill, 2))
		WHEN 'CA' THEN SUM(ROUND(poItem.unitCost * poItem.qtyToFulfill / po.currencyRate, 2))
		WHEN 'AU' THEN SUM(ROUND(poItem.unitCost * poItem.qtyToFulfill / po.currencyRate, 2))
		ELSE 0.0
	END AS currencySubTotal,

	CASE COALESCE(UPPER(sysProperties.sysValue), 'US')
		WHEN 'US' THEN SUM(poItem.unitCost / po.currencyRate * poItem.qtyToFulfill)
		WHEN 'CA' THEN SUM(poItem.unitCost * poItem.qtyToFulfill / po.currencyRate)
		WHEN 'AU' THEN SUM(poItem.unitCost * poItem.qtyToFulfill / po.currencyRate)
		ELSE 0.0
	END AS currencySubTotalNotRounded, COALESCE(po.totalTax,0.0) AS totalTax,
	SUM(ROUND(poItem.unitCost * poItem.qtyToFulfill, 2)) AS subtotalRounded,
    SUM(poItem.unitCost * poItem.qtyToFulfill) AS subtotal,

	/*Use in US only*/
    SUM(ROUND(poItem.unitCost / po.currencyRate * poItem.qtyToFulfill, 2)) * po.currencyRate AS USTotal,
    SUM(poItem.unitCost / po.currencyRate * poItem.qtyToFulfill) * po.currencyRate AS USTotalNotRounded,
    company.ABN, homeCurrency.symbol AS homeCurrencySymbol

FROM po
    INNER JOIN poItem ON po.id = poItem.poId
    INNER JOIN vendor ON po.vendorId = vendor.id
    INNER JOIN company ON company.id = 1
    LEFT OUTER JOIN stateConst AS shipState ON po.shipToStateId = shipState.id
    LEFT OUTER JOIN stateConst AS billState ON po.remitStateId = billState.id
    LEFT OUTER JOIN countryConst AS shipCountry ON po.shipToCountryId = shipCountry.id
    LEFT OUTER JOIN countryConst AS billCountry ON po.remitCountryId = billCountry.id
    LEFT OUTER JOIN sysProperties ON sysProperties.sysKey = 'QBVersion'
    LEFT OUTER JOIN currency ON vendor.currencyId = currency.id
    LEFT JOIN currency AS homeCurrency ON homeCurrency.homeCurrency = 1

WHERE po.num = $P{poNum}

GROUP BY po.vendorContact, po.vendorSo, po.vendorId, po.shipToZip, shipCountry.name, po.shipToName, po.shipToAddress,
		 po.shipToCity, shipState.code, po.remitZip, billCountry.name, po.currencyRate, currency.code,
         currency.symbol, po.remitToName, po.remitAddress, po.remitCity, billState.code, po.note, po.id, po.totalTax,
		 sysProperties.sysValue, company.ABN, homeCurrency.symbol]]>
	</queryString>
